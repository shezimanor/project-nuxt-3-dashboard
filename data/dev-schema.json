{
  "type": "object",
  "properties": {
    "basicJSON": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string",
          "default": "Empty Title",
          "ui": {
            "label": "產品標題",
            "widget": "input",
            "widgetConfig": {
              "placeholder": "請輸入產品標題"
            }
          },
          "rules": {
            "required": true,
            "maxLength": 30
          }
        },
        "description": {
          "type": "string",
          "default": "",
          "ui": {
            "label": "產品標題",
            "widget": "textarea",
            "widgetConfig": {
              "placeholder": "請輸入產品描述(非必填)"
            }
          },
          "rules": {
            "maxLength": 300
          }
        }
      },
      "ui": {
        "label": "基本資訊"
      }
    },
    "prototypeJSON": {
      "type": "object",
      "properties": {
        "others": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "level": {
                "type": "string",
                "default": "等級一",
                "ui": {
                  "label": "等級",
                  "widget": "input",
                  "widgetConfig": {
                    "placeholder": "請輸入等級"
                  }
                },
                "rules": {
                  "required": true
                }
              },
              "comment": {
                "type": "string",
                "default": "良好",
                "ui": {
                  "label": "評價",
                  "widget": "input",
                  "widgetConfig": {
                    "placeholder": "請輸入評價"
                  }
                },
                "rules": {
                  "required": true
                }
              },
              "manyThings": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "default": "你的名字",
                      "ui": {
                        "label": "名字",
                        "widget": "input",
                        "widgetConfig": {
                          "placeholder": "請輸入名字"
                        }
                      },
                      "rules": {
                        "required": true
                      }
                    },
                    "job": {
                      "type": "string",
                      "default": "工程師",
                      "ui": {
                        "label": "工作",
                        "widget": "input",
                        "widgetConfig": {
                          "placeholder": "請輸入工作"
                        }
                      },
                      "rules": {
                        "required": true
                      }
                    }
                  }
                },
                "ui": {
                  "label": "陣列下層又是陣列",
                  "widget": "array-tabs"
                }
              }
            }
          },
          "ui": {
            "label": "其他說明",
            "widget": "array-tabs"
          }
        },
        "colors": {
          "type": "array",
          "default": ["紅"],
          "ui": {
            "label": "顏色",
            "widget": "multiple-select",
            "widgetConfig": {
              "options": ["紅", "橙", "黃", "綠", "藍"],
              "placeholder": "請輸入顏色",
              "searchablePlaceholder": "搜尋顏色"
            }
          },
          "rules": {
            "required": true,
            "maxLength": 3
          }
        },
        "productName": {
          "type": "string",
          "default": "Empty Title",
          "ui": {
            "label": "產品標題",
            "widget": "input",
            "widgetConfig": {
              "placeholder": "請輸入產品標題"
            }
          },
          "rules": {
            "required": true,
            "maxLength": 30
          }
        },
        "productPrice": {
          "type": "number",
          "default": 100,
          "ui": {
            "label": "產品價格",
            "widget": "input",
            "widgetConfig": {
              "placeholder": "請輸入產品價格"
            }
          },
          "rules": {
            "required": true,
            "minValue": 1
          }
        },
        "productQuantity": {
          "type": "number",
          "ui": {
            "label": "產品數量",
            "widget": "input",
            "widgetConfig": {
              "placeholder": "請輸入產品數量"
            }
          },
          "rules": {
            "required": true,
            "minValue": 30
          }
        },
        "isOnSale": {
          "type": "boolean",
          "default": true,
          "ui": {
            "label": "上架狀態",
            "widget": "toggle",
            "widgetConfig": {}
          }
        },
        "productOrigin": {
          "type": "array",
          "default": ["台北市"],
          "ui": {
            "label": "產品產地",
            "widget": "multiple-select",
            "widgetConfig": {
              "options": [
                "台北市",
                "桃園市",
                "新竹市",
                "台中市",
                "新北市",
                "高雄市"
              ],
              "placeholder": "請選擇產地",
              "searchablePlaceholder": "搜尋"
            }
          },
          "rules": {
            "required": true
          }
        },
        "oris": {
          "type": "array",
          "default": ["台北市"],
          "items": {
            "type": "string",
            "default": "台北市",
            "ui": {
              "label": "產品產地",
              "widget": "select",
              "widgetConfig": {
                "options": [
                  "台北市",
                  "桃園市",
                  "新竹市",
                  "台中市",
                  "新北市",
                  "高雄市"
                ],
                "placeholder": "請選擇產地",
                "searchablePlaceholder": "搜尋"
              }
            },
            "rules": {
              "required": true,
              "url": true
            }
          },
          "ui": {
            "label": "產地們"
          },
          "rules": {
            "minLength": 3
          }
        },
        "newUrls": {
          "type": "array",
          "default": [{ "url": "https://www.google.com" }],
          "items": {
            "type": "object",
            "properties": {
              "url": {
                "type": "string",
                "default": "https://www.google.com",
                "ui": {
                  "label": "網址",
                  "widget": "input",
                  "widgetConfig": {
                    "placeholder": "請輸入網址"
                  }
                },
                "rules": {
                  "required": true,
                  "url": true
                }
              }
            }
          },
          "ui": {
            "label": "多個網址"
          },
          "rules": {
            "minLength": 3
          }
        }
      },
      "ui": {
        "label": "核心資料"
      }
    }
  },
  "ui": {
    "label": "表單容器"
  }
}
